public class UserRestController {

	public String admin() {
		return "<h3>Hello Admin</h3>";
	}
	
	public String user() {
		return "<h3>Hello User</h3>";
	}
	
	public String welcome() {
		return "<h3>Hello</h3>";
	}
	
}
